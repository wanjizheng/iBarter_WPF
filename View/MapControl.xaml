<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:iBarter"
    xmlns:Syncfusion="http://schemas.syncfusion.com/wpf" x:Class="iBarter.MapControl"
    mc:Ignorable="d"
    d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="Grid_MapMain" SizeChanged="Grid_MapMain_SizeChanged">
        <Image HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Stretch="Fill"
               Source="/Images/iBarterMap.png" />
        <Grid x:Name="Grid_Ajir" Margin="506,249,284,191" Width="10" Height="10">
            <Rectangle Fill="Blue"/>
            <Image x:Name="Image_Ajir" HorizontalAlignment="Left" Height="10" Margin="0,0,0,0"
                   VerticalAlignment="Top" Width="10" />
        </Grid>
        <Grid x:Name="Grid_Albresser" Margin="243,354,547,86" Width="10" Height="10">
            <Rectangle Fill="Blue"/>
            <Image x:Name="Image_Albresser" HorizontalAlignment="Left" Height="10" Margin="0,0,0,0"
                   VerticalAlignment="Top" Width="10" />
        </Grid>

    </Grid>
</UserControl>